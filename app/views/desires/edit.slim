= react_component 'StaticDesireForm',
  url: desire_path(@desire),
  desire: {author: {name: @desire.user.name,
                    url: user_path(@desire.user),
                    avatar: @desire.user.avatar.to_s},
          city_id: @desire.user.city_id,
          time: l(@desire.created_at),
          title: @desire.title,
          city_name: @desire.user.city.try(:name),
          text: @desire.text,
          post_type: 'photo',
          photos: @desire.photos.map() do |p| {id: p.id, url: p.url} end,
  }